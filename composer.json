{
    "name": "whiterabbit/cakephp4-soft-delete",
    "description": "SoftDelete plugin updated for CakePHP >= 3.10",
    "keywords": ["cakephp", "cakephp 3", "plugin", "soft", "delete", "deletable"],
    "homepage": "https://github.com/EnterpriseConsultingDeveloper/cakephp4-soft-delete",
    "type": "cakephp-plugin",
    "license": "MIT",
    "support": {
        "source": "https://github.com/EnterpriseConsultingDeveloper/cakephp4-soft-delete"
    },
    "require": {
        "php": ">=8.0",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": ">=4.0"
    },
    "require-dev": {
        "cakephp/cakephp": "4.5.*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SoftDelete\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoftDelete\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    }
}
